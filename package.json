{
  "name": "vscode-markdown-to-clipboard",
  "displayName": "Markdown to clipboard",
  "description": "An extension that renders your markdown file to clipboard as rich text, so that you can paste it to any ord processor.",
  "version": "0.0.2",
  "publisher": "mlewand",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mlewand/vscode-markdown-to-clipboard.git"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-markdown-to-clipboard"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "vscode-markdown-to-clipboard",
        "title": "Copy markdown to clipboard as Rich Text"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/html-to-rtf": "^1.3.0",
    "@types/html-to-text": "^5.1.0",
    "@types/marked": "^4.0.7",
    "@types/mocha": "^2.2.32",
    "@types/node": "^12.20.55",
    "@types/sinon": "^2.1.2",
    "@types/sinon-chai": "^2.7.27",
    "chai": "^3.5.0",
    "mocha": "^2.3.3",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "typescript": "^4.8.4",
    "vscode": "^1.0.0",
    "vscode-test-content": "^1.2.0"
  },
  "dependencies": {
    "html-to-rtf": "^2.0.1",
    "html-to-text": "^3.3.0",
    "marked": "^4.1.1",
    "native-clip": "^0.5.3"
  },
  "optionalDependencies": {
    "objc": "^0.23.0",
    "win-clipboard": "0.0.6"
  }
}
